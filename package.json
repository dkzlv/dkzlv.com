{
  "name": "dkzlv-blog",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "open http://blog:3000/ & sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "new": "node scripts/newPost/index.js"
  },
  "dependencies": {
    "@mojs/core": "git+ssh://git@github.com/dkzlv/mojs",
    "@sindresorhus/slugify": "^0.11.0",
    "compression": "^1.7.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "feed": "^4.0.0",
    "fingerprintjs2": "^2.1.0",
    "js-yaml": "^3.13.1",
    "marked": "^0.8.2",
    "node-polyglot": "^2.3.1",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/express": "^4.17.1",
    "@types/js-yaml": "^3.12.1",
    "@types/marked": "^0.7.3",
    "@types/node-polyglot": "^0.4.34",
    "@typescript-eslint/parser": "^2.1.0",
    "autoprefixer": "^9.6.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.1",
    "modularscale-sass": "^3.0.8",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "postcss-load-config": "^2.1.0",
    "prettier": "^2.0.2",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sapper": "^0.27.11",
    "sass-mq": "^5.0.1",
    "svelte": "^3.20.1",
    "svelte-preprocess": "^3.6.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.2"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "no-console": 0,
      "no-unused-vars": 0
    }
  },
  "eslintIgnore": [
    "build/",
    "node_modules/",
    "__sapper__/",
    "cypress/"
  ],
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  }
}
